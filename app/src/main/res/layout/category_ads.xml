<?xml version="1.0" encoding="utf-8"?>
    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".CategoryAds">


    <ImageView
        android:id="@+id/imageView38"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/rectangle_3578" />

    <ImageView
        android:id="@+id/btnBackImg3"
        android:layout_width="41dp"
        android:layout_height="26dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView38"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView38"
        app:layout_constraintVertical_bias="0.226"
        app:srcCompat="@drawable/chevron_left" />

    <TextView
        android:id="@+id/btnBackToCtg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="252dp"
        android:text="Categories"
        android:textAlignment="center"
        android:textColor="#FFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView38"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="@+id/imageView38"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.216" />

    <TextView
        android:id="@+id/CategoryText"
        android:layout_width="149dp"
        android:layout_height="38dp"
        android:text="Bikes"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/searchView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.733"
        app:layout_constraintStart_toEndOf="@+id/btnBackToCtg"
        app:layout_constraintTop_toTopOf="@+id/imageView38"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/maxPriceLabel"
        android:layout_width="152dp"
        android:layout_height="44dp"
        android:layout_marginTop="16dp"
        android:max="100000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/minPriceSeekBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.187"
        app:layout_constraintStart_toEndOf="@+id/minPriceLabel"
        app:layout_constraintTop_toBottomOf="@+id/imageView38"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/minPriceLabel"
        android:layout_width="152dp"
        android:layout_height="44dp"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/minPriceSeekBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.362"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView38"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
            android:id="@+id/textView42"
            android:layout_width="64dp"
            android:layout_height="30dp"
            android:text="Price "
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.045"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.161" />

    <TextView
        android:id="@+id/textView44"
        android:layout_width="60dp"
        android:layout_height="30dp"
        android:text="Lowest"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView64"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView64"
        android:layout_width="64dp"
        android:layout_height="30dp"
        android:text="Highest"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/recyclerViewCtg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.046"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <SeekBar
        android:id="@+id/minPriceSeekBar"
        android:layout_width="291dp"
        android:layout_height="30dp"
        android:max="10000000"
        android:min="10000"
        android:progress="100000"
        app:layout_constraintBottom_toTopOf="@+id/maxPriceSeekBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.866"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView38"
        app:layout_constraintVertical_bias="1.0" />

    <SeekBar
        android:id="@+id/maxPriceSeekBar"
        android:layout_width="291dp"
        android:layout_height="30dp"
        android:layout_marginTop="80dp"
        android:max="100000000"
        android:min="50000"
        android:progress="50000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.866"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView38"
        app:layout_constraintBottom_toTopOf="@+id/recyclerViewCtg"/>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerViewCtg"
        android:layout_width="409dp"
        android:layout_height="517dp"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/maxPriceSeekBar" />

    <androidx.appcompat.widget.SearchView
        android:id="@+id/searchViewCtg"
        android:layout_width="367dp"
        android:layout_height="43dp"
        android:layout_margin="8dp"
        android:layout_marginTop="56dp"
        android:background="@drawable/rounded_search"
        app:closeIcon="@drawable/search_view_bg"
        app:iconifiedByDefault="false"
        app:layout_constraintBottom_toBottomOf="@+id/imageView38"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.772"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:queryBackground="@color/white"
        app:queryHint="Search Here ..." />


</androidx.constraintlayout.widget.ConstraintLayout>